<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="InspectionIdentification" namespace="/inspectionIdentific"
		extends="struts-default">
		<action name="EntrustmentBookManagement_*"
			class="com.xsjsglxt.action.InspectionIdentification.InspectionIdentificationAction"
			method="{1}">
			<result name="EntrustmentBookManagement">/WEB-INF/view/InspectionIdentification/EntrustmentBookManagement.jsp
			</result>

			<result name="exportTranceCheckBook" type="stream">
				<param name="contentType">application/octet-stream</param>
				<param name="inputName">inputStream</param>
				<param name="contentDisposition">
					filename=${fileName}
				</param>
				<param name="bufferSize">1024</param>

			</result>
		</action>
	</package>
</struts>
