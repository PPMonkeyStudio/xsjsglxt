<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="User" namespace="/user" extends="struts-default">
		<action name="User_*" class="com.xsjsglxt.action.User.UserAction"
			method="{1}">
			<result name="index">/WEB-INF/view/index.jsp
			</result>
			<result name="navbar">/WEB-INF/view/navbar.jsp
			</result>
			<result name="logoutSuccess">
			    /login.jsp
			</result>
			<result name="skipToUser">
			    /WEB-INF/view/User/UserManagerIndex.jsp
			</result>
		</action>
		
		<action name="Meeting_*" class="com.xsjsglxt.action.User.MeetingAction" method="{1}">
			<result name="intoMeeting">/WEB-INF/view/User/MeetRecords.jsp</result>
		</action>
		<action name="Memorabilia_*" class="com.xsjsglxt.action.User.MemorabiliaAction" method="{1}">
			<result name="skipSuccess">/WEB-INF/view/User/Memorabilia.jsp</result>
		</action>
	</package>
</struts>
