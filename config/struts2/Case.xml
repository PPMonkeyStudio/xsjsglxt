<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="Case" namespace="/case" extends="struts-default">
		<action name="Case_*" class="com.xsjsglxt.action.Case.SenceAction"
			method="{1}">
			<result name="page_SiteManagement">/WEB-INF/view/Case/SiteManagement.jsp</result>
			<result name="page_Sneceinput">/WEB-INF/view/Case/SneceInput.jsp</result>
			<result name="page_CaseMergerList">/WEB-INF/view/Case/CaseMergerList.jsp</result>
			<result name="page_CaseDetails">/WEB-INF/view/Case/CaseDetails.jsp</result>
			<result name="page_CaseMerger">/WEB-INF/view/Case/CaseMerger.jsp</result>
			<result name="page_CaseMaterials">/WEB-INF/view/Case/CaseMaterials_image.jsp</result>
			<result name="page_Handle">/WEB-INF/view/Case/HandleList.jsp</result>
			<result name="page_intoDetails">/WEB-INF/view/Case/CaseDetails_table.jsp</result>
			<result name="page_printPage">/WEB-INF/view/Case/printPage.jsp</result>
			<result name="intoEvidence">/WEB-INF/view/Case/EvidenceManager.jsp</result>
		</action>
		<action name="Resevidence_*" class="com.xsjsglxt.action.Case.ResevidenceAction"
			method="{1}">
		</action>
		<action name="Parallel_*" class="com.xsjsglxt.action.Case.ParallelAction"
			method="{1}">
		</action>
		<action name="LostMobilephone_*" class="com.xsjsglxt.action.Case.LostMobilephoneAction"
			method="{1}">
		</action>
		<action name="LostComputer_*" class="com.xsjsglxt.action.Case.LostComputerAction"
			method="{1}">
		</action>
		<action name="Lost_*" class="com.xsjsglxt.action.Case.LostAction"
			method="{1}">
		</action>
		<action name="Image_*" class="com.xsjsglxt.action.Case.ImageAction"
			method="{1}">
		</action>
		<action name="Handle_*" class="com.xsjsglxt.action.Case.HandleAction"
			method="{1}">
		</action>
		
		<!-- gxr重构破案模块添加代码 -->
		<action name="BreakeCase_*" class="com.xsjsglxt.action.Case.BreakecaseAction"
			method="{1}">
			<result name="page_BreakCaseList">/WEB-INF/view/Case/BreakCaseList.jsp</result>
		</action>
	</package>
</struts>
